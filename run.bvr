IF NOT runTag THEN SET runTag main
STEP START Cloning ${runRepo}:${runTag}
   IF runRepoProject THEN GCLOUD CLONE BRANCH OF ${runTag} SOURCE REPO ${runRepo} IN PROJECT ${runRepoProject}
   IF NOT repoProject THEN GIT CLONE ${runGit} ${runRepo} ${runTag}
STEP END

LOG Executing beaver script ${runScript} in ${runRepo}
EXECUTE ${runRepo}/${runScript} IN ${runRepo}
